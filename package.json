    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.7",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
