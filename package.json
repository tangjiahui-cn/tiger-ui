{
  "name": "tiger-ui",
  "version": "0.0.0",
  "description": "A React components library.",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "dev": "cross-env webpack serve -c scripts/server/index.js",
    "build": "pnpm clearn && cross-env webpack -c scripts/build/index.js && pnpm tsc",
    "build-rollup": "pnpm clearn && rollup -c scripts/build/rollup/rollup.build.js --bundleConfigAsCjs",
    "tsc": "tsc -p ./tsconfig.json",
    "clearn": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangjiahui-cn/tiger-ui.git"
  },
  "keywords": [
    "React",
    "components"
  ],
  "author": "tangjiahui-cn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tangjiahui-cn/tiger-ui/issues"
  },
  "homepage": "https://github.com/tangjiahui-cn/tiger-ui#readme",
  "engines": {
    "node": ">= 14.x || 16.x.x"
  },
  "peerDependencies": {
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "cross-env": "^7.0.3",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-alias": "^5.0.0",
