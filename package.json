{
  "name": "tiger-ui",
  "version": "0.0.5",
  "description": "A React components library.",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "main": "umd/index.js",
  "author": "tangjiahui-cn",
  "homepage": "https://github.com/tangjiahui-cn/tiger-ui#readme",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env mode=development esno scripts/server/index.ts",
    "build:types": "tsc -p ./tsconfig.lib.json",
    "build:umd": "cross-env target=umd esno scripts/build/index.ts",
    "build:cjs": "cross-env target=cjs esno scripts/build/index.ts",
    "build:esm": "rimraf es && cross-env target=esm esno scripts/build/index.ts",
    "build": "cross-env target=all esno scripts/build/index.ts",
    "docs": "dumi dev",
    "docs:build": "dumi build",
    "test": "jest -c jest.config.ts --no-cache",
    "fomat-code-all": "eslint --ext .js,.ts,.jsx,.tsx */* --fix && prettier --write .",
    "prepare": "husky install",
    "commit": "git add . && git-cz",
    "publish-npm": "sh scripts/publish/checkLogin.sh && pnpm build && sh scripts/publish/publish.sh"
  },
  "files": [
    "umd",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangjiahui-cn/tiger-ui.git"
  },
  "keywords": [
    "React",
    "components",
    "tiger-ui",
    "library",
    "front-end"
  ],
  "bugs": {
    "url": "https://github.com/tangjiahui-cn/tiger-ui/issues"
  },
  "engines": {
    "node": ">= 14.x || 16.x.x"
  },
  "peerDependencies": {
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.3.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.8.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/speed-measure-webpack-plugin": "^1.3.6",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "class-css": "^1.0.28",
    "classnames": "^2.3.2",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "cssnano": "^6.0.3",
    "dumi": "^2.2.17",
    "dumi-theme-lobehub": "^1.7.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "esno": "^4.0.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-css": "^6.0.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.7.5",
    "moment": "^2.30.1",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.2.4",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.9.5",
    "rollup-plugin-postcss": "^4.0.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.3",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  },
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint */* --fix",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
